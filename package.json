{
  "name": "circuit-ui",
  "version": "0.1.0",
  "description": "Interactive circuit board-style canvas for portfolio displays",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "vite --host",
    "demo": "vite --host --open /",
